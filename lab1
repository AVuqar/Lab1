import math
n1=((math.log2(5)*math.sqrt(5))-(math.pow(5,1/3)*math.log(5)/math.log(3)))/(1-0.1845*(math.sin(1)+2*math.cos(1)))
n2=math.exp(-2)*(1/math.tan(math.cos(-4/7)/2))
if n1*n2 <0.1:
    s=math.sqrt(abs(n1*n2))
else:
    s=math.sqrt(abs(n1+n2))
print(f"n1={n1}")
print(f"n2={n2}")
print(f"s={s}")
